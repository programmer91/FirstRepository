<hibernate-mapping>
    <class name="com.mss.mirage.crm.attachments.AttachmentAction" table="tblPrjAttachments" entity-name="ProjectIssuesData">
        <id name="id" column="Id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property name="objectId" column="ObjectId"/>
        <property name="objectType" column="ObjectType"/>
        <property name="attachmentName" column="AttachmentType"/>
        <property name="attachmentLocation" column="Path"/>        
        <property name="dateUploaded" column="DateOfUpload"/>
    </class>
</hibernate-mapping>